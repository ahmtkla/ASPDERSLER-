@using proje3.Models.Entity;
@model List<TBLMUSTERİLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>MUSTERİ ID</th>
        <th>MUSTERİ ADI</th>
        <th>MUSTERİ SOYADI</th>

    </tr>
    <tbody>
        @foreach (var mus in Model)
        {
            <tr>
                <th>@mus.MUSTERIIDA</th>
                <td>@mus.MUSTERİAD</td>
                <td>@mus.MUSTERİSOYAD</td>

            </tr>
        }
    </tbody>
</table>
<a href="/Musteri/YeniMusteri/" class="btn btn-primary">YENİ MÜŞTERİ EKLE</a>

